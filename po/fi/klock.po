#: main.cpp:580
#, c-format
msgid ""
"Usage: %s -install|-setup|-test|-desc|-preview wid|-inroot\n"
"       [-corners xxxx] [-delay num] [-lock] [-allow-root] [-nice num]\n"
msgstr ""
"Käyttö: %s -install|-setup|-test|-desc|-preview wid|-inroot\n"
"       [-corners xxxx] [-delay num] [-lock] [-allow-root] [-nice num]\n"

#: main.cpp:583
msgid ""
"  -corners xxxx     Placing cursor in corner performs action:\n"
"                     x = i  no action (ignore)\n"
"                     x = s  save screen\n"
"                     x = l  lock screen\n"
"                    order: top-left, top-right, bottom-left, bottom-right\n"
"  -delay num        Amount of idle time before screen saver\n"
"                     starts  (default 10min)\n"
"  -desc             Print the screen saver's description to stdout\n"
"  -install          Install screen saver\n"
"  -lock             Require password to stop screen saver\n"
"  -allow-root       Accept root password to unlock\n"
"  -nice num         Run with specified nice value\n"
"\n"
"  -preview wid      Run in the specified XWindow\n"
"  -inroot           Run in the root window\n"
"  -setup            Setup screen saver\n"
"  -test             Invoke the screen saver immediately\n"
msgstr ""
"  -corners xxxx     Kursorin sijoittaminen nurkkaan suorittaa:\n"
"                     x = i  ei mitään\n"
"                     x = s  tallenna näyttö\n"
"                     x = l  lukitse näyttö\n"
"                    järjestys: ylävasen, yläoikea, alavasen, alaoikea\n"
"  -delay num        Aika joka odotetaan ennenkuin näytönsäästäjä\n"
"                     käynnistetään  (oletus 10min)\n"
"  -desc             Tulosta näytönsäästäjän kuvaus stdout:n\n"
"  -install          Asenna näytönsäästäjä\n"
"  -lock             Vaadi salasana näytönsäästäjän lopettamiseksi\n"
"  -allow-root       Hyväksy root-salasana avaamiseen\n"
"  -nice num         Ajaa halutulla nice arvolla\n"
"\n"
"  -preview wid      Aja määritellyssä X-Ikkunassa\n"
"  -inroot           Aja root-ikkunassa\n"
"  -setup            Aseta näytönsäästäjää\n"
"  -test             Käynnistä näytönsäästäjää heti\n"

