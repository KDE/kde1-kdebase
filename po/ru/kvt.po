#: main.C:1478
msgid "Permitted arguments are:\n"
msgstr "Разрешенные аргументы:\n"

#: main.C:1479
msgid ""
"-e <command> <arg> ...    execute command with ars - must be last argument\n"
msgstr ""
"-e <command> <arg> ... запустить команду с параметрами arg - должна быть\n"
"                       последней опцией\n"

#: main.C:1480
msgid "-display <name>           specify the display (server)\n"
msgstr "-display <name>           использовать дисплей (сервер) <name>\n"

#: main.C:1481
msgid "-vt_geometry <spec>       the initial vt window geometry\n"
msgstr "-vt_geometry <spec>       начальные размеры окна\n"

#: main.C:1482
msgid "-print-pipe <name>        specify pipe for vt100 printer\n"
msgstr ""
"-print-pipe <name>        использовать канал <pipe> для принтера vt100\n"

#: main.C:1483
msgid "-vt_bg <colour>           background color\n"
msgstr "-vt_bg <colour>           фоновый цвет\n"

#: main.C:1484
msgid "-vt_fg <colour>           foreground color\n"
msgstr "-vt_fg <colour>           цвет переднего плана\n"

#: main.C:1485
msgid "-vt_font <fontname>       normal font\n"
msgstr "-vt_font <fontname>       нормальный шрифт\n"

#: main.C:1486
msgid "-no_font_hack             turn off Qt font bug workaround\n"
msgstr "-no_font_hack             выключает обход ошибки в Qt\n"

#: main.C:1487
msgid "-vt_size <size>           tiny, small, normal, large, huge\n"
msgstr "-vt_size <size>           tiny, small, normal, large, huge\n"

#: main.C:1488
msgid "-linux                    set up kvt to mimic linux-console\n"
msgstr "-linux                    эмуляция линуксового терминала\n"

#: main.C:1489
msgid "-no_menubar               hide the menubar\n"
msgstr "-no_menubar               спрятать меню\n"

#: main.C:1490
msgid "-no_scrollbar             hide the scrollbar\n"
msgstr "-no_scrollbar             спрятать полосы прокрутки\n"

#: main.C:1491
msgid ""
"-T <text>                 text in window titlebar \n"
"                          (Obsolete. Use -caption instead)\n"
msgstr ""
"-T <text>                  установить заголовок <text>\n"
"                           (Устарела. Используйте -caption)\n"

#: main.C:1493
msgid "-tn <TERM>                Terminal type. Default is xterm.\n"
msgstr "-tn <TERM>                Тип терминала, по умолчанию xterm.\n"

#: main.C:1494
msgid "-C                        Capture system console message\n"
msgstr ""
"-C                        Перехватывать сообщения с системной консоли\n"

#: main.C:1495
msgid "-n <text>                 name in icon or icon window\n"
msgstr "-n <text>                 имя пиктограммы\n"

#: main.C:1496
msgid ""
"-7                        run in 7 bit mode\n"
"-8                        run in 8 bit mode\n"
msgstr ""
"-7                         запускаться в 7 битном режиме\n"
"-8                         запускаться в 8 битном режиме\n"

#: main.C:1498
msgid ""
"-ls                       initiate kvt's shell as a login shell\n"
"-ls-                      initiate kvt's shell as a non-login shell "
"(default)\n"
msgstr ""
"-ls                        запрашивать имя и пароль\n"
"-ls-                       не запрашивать имени и пароля (по умолчанию)\n"

#: main.C:1500
msgid ""
"-meta <arg>               handle Meta with ESCAPE prefix, 8THBIT set, or "
"ignore\n"
msgstr ""
"-meta                       обрабатывать клавишу META с префиксом ESCAPE,\n"
"                           установите 8-ми битный режим, или опция будет\n"
"                           проигнорирована\n"

#: main.C:1501
msgid "-sl <number>              save number lines in scroll-back buffer\n"
msgstr "-sl <number>              сохранять number количество строк в буфере\n"

#: main.C:1502
msgid ""
"-pageup <keysym>          use hot key alt-keysym to scroll up through the "
"buffer\n"
"-pagedown <keysym>        use hot key alt-keysym to scroll down through "
"buffer\n"
msgstr ""
"-pageup <keysym>           использовать клавишу alt-keysym для прокрутки "
"вверх\n"
"-pagedown <keysym>         использовать клавишу alt-keysym для прокрутки "
"вниз\n"

#: main.C:1506
msgid ""
"-grk9\t\tgreek kbd = ELOT 928 (default)\n"
"-grk4\t\tgreek kbd = IBM 437\n"
msgstr ""
"-grk9\t\tгреческая клавиатура ELOT 928 (по умолчанию)\n"
"-grk4\t\tгреческая клавиатура IBM 437\n"
