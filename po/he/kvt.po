#: main.C:1439
msgid "Permitted arguments are:\n"
msgstr ":םה םיירשפאה םיטנמוגראה\n"

#: main.C:1440
msgid ""
"-e <command> <arg> ...    execute command with ars - must be last argument\n"
msgstr ""
"-e <הדוקפ> <םיטנמוגרא>...ןורחאה טנמוגראה תויהל בייח .גראה םע הדוקפה תא ץרה\n"

#: main.C:1441
msgid "-display <name>           specify the display (server)\n"
msgstr "-display <הגוצת םש>   לנימרטה הילע ץורי רשא הגוצתה תא רידגמ\n"

#: main.C:1442
msgid "-vt_geometry <spec>       the initial vt window geometry\n"
msgstr "-vt_geometry<הרדגה>   לנימרטה לש םירוטהו תורושה תא רידגמ\n"

#: main.C:1443
msgid "-print-pipe <name>        specify pipe for vt100 printer\n"
msgstr ""

#: main.C:1444
msgid "-vt_bg <colour>           background color\n"
msgstr "-vt_bg <עבצ>                      עקר עבצ\n"

#: main.C:1445
msgid "-vt_fg <colour>           foreground color\n"
msgstr "-vt_fg <עבצ>                     טסקט עבצ\n"

#: main.C:1446
msgid "-vt_font <fontname>       normal font\n"
msgstr "-vt_font <ןפוג םש>    ליגרה ןפוגה םש\n"

#: main.C:1447
msgid "-no_font_hack             turn off Qt font bug workaround\n"
msgstr ""

#: main.C:1448
msgid "-vt_size <size>           tiny, small, normal, large, huge\n"
msgstr "-vt_size <size>          tiny, small, normal, large, huge\n"

#: main.C:1449
msgid "-linux                    set up kvt to mimic linux-console\n"
msgstr "-linux     סקוניל לש לוסנוקה תא תוקחל לנימרטה תנכותל רשפאמ\n"

#: main.C:1450
msgid "-no_menubar               hide the menubar\n"
msgstr "-no_menubar              םיטירפת אלל\n"

#: main.C:1451
msgid "-no_scrollbar             hide the scrollbar\n"
msgstr "-no_scrollbar            הלילג ספ אלל\n"

#: main.C:1452
msgid ""
"-T <text>                 text in window titlebar \n"
"                          (Obsolete. Use -caption instead)\n"
msgstr ""
"-T <טסקט>                 ןולחה תרתוכב עיפויש טסקט \n"
"                םוקמב -caption ב שמתשהל שי - ןשוימ \n"

#: main.C:1454
msgid "-tn <TERM>                Terminal type. Default is xterm.\n"
msgstr "-tn <לנימרט גוס>             xterm לדחמה תרירב-לנימרט גוס\n"

#: main.C:1455
msgid "-C                        Capture system console message\n"
msgstr "-C                               לוסנוקה לש תועדוה טולק\n"

#: main.C:1456
msgid "-n <text>                 name in icon or icon window\n"
msgstr "-n <text>                למס ןולחב וא למס רותב םש\n"

#: main.C:1457
msgid ""
"-7                        run in 7 bit mode\n"
"-8                        run in 8 bit mode\n"
msgstr ""
"-7                        טיב 7 לנימרטכ הצרה\n"
"-8                        טיב 8 לנימרטכ הצרה\n"

#: main.C:1459
msgid ""
"-ls                       initiate kvt's shell as a login shell\n"
"-ls-                      initiate kvt's shell as a non-login shell "
"(default)\n"
msgstr ""
"-ls                       login shell תכרעמל הסינכ תפטעמב שומיש\n"
"-ls-                         non-login shell הליגר תפטעמב שומיש \n"

#: main.C:1461
msgid ""
"-meta <arg>               handle Meta with ESCAPE prefix, 8THBIT set, or "
"ignore\n"
msgstr ""
"-meta <arg>               handle Meta with ESCAPE prefix, 8THBIT set, or "
"ignore\n"

#: main.C:1462
msgid "-sl <number>              save number lines in scroll-back buffer\n"
msgstr "-sl <רפסמ>               הלילג תרטמל רוחאל ורמשי רשא תורושה רפסמ\n"

#: main.C:1463
msgid ""
"-pageup <keysym>          use hot key alt-keysym to scroll up through the "
"buffer\n"
"-pagedown <keysym>        use hot key alt-keysym to scroll down through "
"buffer\n"
msgstr ""
"-pageup <שקמ לומיס>        הלעמל דומע תלילגל ןיוצמה שקמה+טלאה שקמב שמתשה\n"
" -pagedown <שקמ לומיס>        הטמל דומע תלילגל ןיוצמה שקמה+טלאה שקמב שמתשה\n"

#: main.C:1467
msgid ""
"-grk9\t\tgreek kbd = ELOT 928 (default)\n"
"-grk4\t\tgreek kbd = IBM 437\n"
msgstr ""
"-grk9\t\tgreek kbd = ELOT 928 (default)\n"
"-grk4\t\tgreek kbd = IBM 437\n"
