#: main.cpp:469
#, c-format
msgid ""
"Usage: %s -install|-setup|-test|-desc|-preview wid|-inroot\n"
"       [-corners xxxx] [-delay num] [-lock] [-nice num]\n"
msgstr ""
"Användning: %s -install|-setup|-test|-desc|-preview wid|-inroot\n"
"       [-corners xxxx] [-delay num] [-lock] [-nice num]\n"

#: main.cpp:472
msgid ""
"  -corners xxxx     Placing cursor in corner performs action:\n"
"                     x = i  no action (ignore)\n"
"                     x = s  save screen\n"
"                     x = l  lock screen\n"
"                    order: top-left, top-right, bottom-left, bottom-right\n"
"  -delay num        Amount of idle time before screen saver\n"
"                     starts  (default 10min)\n"
"  -desc             Print the screen saver's description to stdout\n"
"  -install          Install screen saver\n"
"  -lock             Require password to stop screen saver\n"
"  -nice num         Run with specified nice value\n"
"\n"
"  -preview wid      Run in the specified XWindow\n"
"  -inroot           Run in the root window\n"
"  -setup            Setup screen saver\n"
"  -test             Invoke the screen saver immediately\n"
msgstr ""
"  -corners xxxx     Om markoören placeras i hörnet görs:\n"
"                     x = i inget (ignorera)\n"
"                     x = s släck skärmen\n"
"                     x = l lås skärmen\n"
"                    ordning: övre vänstra, övre högra, nedre vänstra, nedre "
"högra\n"
"  -delay num        Fördröjning innan skärmsläckaren startat\n"
"                     (standardvärdet är 10 min)\n"
"  -desc             Skriv skärmsläckarens beskrivning till stdout\n"
"  -install          Installera skärmsläckaren\n"
"  -lock             Kräv lösenord för att stoppa skärmsläckaren\n"
"  -nice num         Kör med speciellt nice-värde\n"
"\n"
"  -preview wid      Kör i specifierat X-fönster\n"
"  -inroot           Kör i rotfönstret\n"
"  -test             Starta skärmsläckaren med en gång\n"

