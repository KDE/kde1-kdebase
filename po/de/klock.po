#: main.cpp:420
#, c-format
msgid ""
"Usage: %s -install|-setup|-test|-desc|-preview wid\n"
"       [-corners xxxx] [-delay num] [-lock] [-nice num]\n"
msgstr ""
"Benutzung: %s -install|-setup|-test|-desc|-preview wid\n"
"       [-corners xxxx] [-delay zahl] [-lock] [-nice zahl]\n"

#: main.cpp:423
msgid ""
"  -corners xxxx     Placing cursor in corner performs action:\n"
"                     x = i  no action (ignore)\n"
"                     x = s  save screen\n"
"                     x = l  lock screen\n"
"                    order: top-left, top-right, bottom-left, bottom-right\n"
"  -delay num        Amount of idle time before screen saver\n"
"                     starts  (default 10min)\n"
"  -desc             Print the screen saver's description to stdout\n"
"  -install          Install screen saver\n"
"  -lock             Require password to stop screen saver\n"
"  -nice num         Run with specified nice value\n"
"\n"
"  -preview wid      Run in the specified XWindow\n"
"  -setup            Setup screen saver\n"
"  -test             Invoke the screen saver immediately\n"
msgstr ""
"  -corners xxxx     Mousezeiger in Ecke führt zu:\n"
"                     x = i  keine Aktion (ignorieren)\n"
"                     x = s  Bildschirm schonen\n"
"                     x = l  Bildschirm verschliessen\n"
"                    Reihenfolge: links-oben, rechts-oben, links-unten, "
"rechts-unten\n"
"  -delay zahl       Zeit, bis der Bildschirmschoner startet\n"
"                     (Normalerweise 10 Minuten)\n"
"  -desc             Gibt den Namen des Bildschirmschoners aus\n"
"  -install          Bildschirmschoner einrichten\n"
"  -lock             Passwortschutz des Bildschirmschoners\n"
"  -nice zahl        Starten mit angegebener Priorität (nice level)\n"
"\n"
"  -preview wid      Läuft in dem angegeben XWindow\n"
"  -setup            Bildschirmschoner einstellen\n"
"  -test             Bildschirmschoner sofort starten\n"

