#: main.C:1478
msgid "Permitted arguments are:\n"
msgstr "Lubatud argumendid on:\n"

#: main.C:1479
msgid ""
"-e <command> <arg> ...    execute command with ars - must be last argument\n"
msgstr ""
"-e <command> <arg> ...    käivita käsk argumentidega - peab olema viimane "
"argument\n"

#: main.C:1480
msgid "-display <name>           specify the display (server)\n"
msgstr "-display <name>           täpsusta ekraan (server)\n"

#: main.C:1481
msgid "-vt_geometry <spec>       the initial vt window geometry\n"
msgstr "-vt_geometry <spec>       esialgne vt akna geomeetria\n"

#: main.C:1482
msgid "-print-pipe <name>        specify pipe for vt100 printer\n"
msgstr "-print-pipe <name>        määra vt100 printeri toru\n"

#: main.C:1483
msgid "-vt_bg <colour>           background color\n"
msgstr "-vt_bg <colour>           tausta värv\n"

#: main.C:1484
msgid "-vt_fg <colour>           foreground color\n"
msgstr "-vt_fg <colour>           esiplaani värv\n"

#: main.C:1485
msgid "-vt_font <fontname>       normal font\n"
msgstr "-vt_font <fontname>       normaalne font\n"

#: main.C:1486
msgid "-no_font_hack             turn off Qt font bug workaround\n"
msgstr "-no_font_hack             Qt fondi vea parandamine keelatud\n"

#: main.C:1487
msgid "-vt_size <size>           tiny, small, normal, large, huge\n"
msgstr "-vt_size <size>           tiny, small, normal, large, huge\n"

#: main.C:1488
msgid "-linux                    set up kvt to mimic linux-console\n"
msgstr ""
"-linux                    seadista kvt kui järeleaimatud linux'i konsool\n"

#: main.C:1489
msgid "-no_menubar               hide the menubar\n"
msgstr "-no_menubar               peida menüüriba\n"

#: main.C:1490
msgid "-no_scrollbar             hide the scrollbar\n"
msgstr "-no_scrollbar             peida kerimisriba\n"

#: main.C:1491
msgid ""
"-T <text>                 text in window titlebar \n"
"                          (Obsolete. Use -caption instead)\n"
msgstr ""
"-T <text>                 tekst akna tiitliribal \n"
"                          (Vananenud. Kasuta selle asemel -caption)\n"

#: main.C:1493
msgid "-tn <TERM>                Terminal type. Default is xterm.\n"
msgstr "-tn <TERM>                Terminali tüüp. Vaikimisi on xterm.\n"

#: main.C:1494
msgid "-C                        Capture system console message\n"
msgstr "-C                        Näita teateid süsteemselt konsoolilt\n"

#: main.C:1495
msgid "-n <text>                 name in icon or icon window\n"
msgstr "-n <text>                 nimi ikoonil või ikooni aknal\n"

#: main.C:1496
msgid ""
"-7                        run in 7 bit mode\n"
"-8                        run in 8 bit mode\n"
msgstr ""
"-7                        7 bitine reziim\n"
"-8                        8 bitine reziim\n"

#: main.C:1498
msgid ""
"-ls                       initiate kvt's shell as a login shell\n"
"-ls-                      initiate kvt's shell as a non-login shell "
"(default)\n"
msgstr ""
"-ls                       käivita kvt shell kui login shell\n"
"-ls-                      käivita kvt shell kui mitte-login shell "
"(vaikimisi)\n"

#: main.C:1500
msgid ""
"-meta <arg>               handle Meta with ESCAPE prefix, 8THBIT set, or "
"ignore\n"
msgstr ""
"-meta <arg>               käsitle Meta't prefiksiga ESCAPE, 8.biti "
"seadmiseks või ignoreeri\n"

#: main.C:1501
msgid "-sl <number>              save number lines in scroll-back buffer\n"
msgstr ""
"-sl <number>              mitu rida salvestada tagasi kerimiseks puhvrisse\n"

#: main.C:1502
msgid ""
"-pageup <keysym>          use hot key alt-keysym to scroll up through the "
"buffer\n"
"-pagedown <keysym>        use hot key alt-keysym to scroll down through "
"buffer\n"
msgstr ""
"-pageup <keysym>          kasuta kombinatsiooni ALT+keysym puhvris üles "
"kerimiseks\n"
"-pagedown <keysym>        kasuta kombinatsiooni ALT+keysym puhvris alla "
"kerimiseks\n"

#: main.C:1506
msgid ""
"-grk9\t\tgreek kbd = ELOT 928 (default)\n"
"-grk4\t\tgreek kbd = IBM 437\n"
msgstr ""
"-grk9\t\tgreek kbd = ELOT 928 (default)\n"
"-grk4\t\tgreek kbd = IBM 437\n"
